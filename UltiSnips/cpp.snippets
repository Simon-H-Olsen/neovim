snippet cout "cout"
std::cout << $1
endsnippet

snippet endl "endl"
std::endl;$1
endsnippet

snippet IO "include IO"
#include <iostream>$1
endsnippet

snippet str "std::string"
std::string("$1")
endsnippet
	
snippet vec "std::vector< >"
std::vector< $1 > $2
endsnippet
	
snippet main "main function template"
int main(int argc, char **args) {}
endsnippet


